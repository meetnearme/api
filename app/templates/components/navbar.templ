package templates

templ NavbarComponent() {
	<div class="navbar bg-primary shadow-xl rounded-box">
		<div class="navbar-start">
			<a
				class="btn btn-ghost text-xl"
				hx-get="/"
				hx-push-url="true"
				hx-target="#main-content"
			>Meet Near Me</a>
		</div>
		<div class="navbar-end">
			<button
				class="btn btn-ghost btn-accent"
				hx-get="/login"
				hx-target="#main-content"
				hx-push-url="true"
			>
				Login
			</button>
		</div>
	</div>
}
