package views

import "github.com/meetnearme/api/functions/lambda/shared"

templ Home(pages []shared.Page) {
	@Layout("Home") {
		<h2 class="title">Examples:</h2>
		<table class="table is-fullwidth"><thead>
	<tr>
		<th>Pattern</th>
		<th>Description</th>
	</tr>
</thead><tbody>
	for _, e := range pages {
		<tr>
			<td><a href={ templ.SafeURL("/"+e.Slug) }>{ e.Name }</a></td>
			<td>{ e.Desc }</td>
		</tr>
	}
</tbody></table>
	}
}

