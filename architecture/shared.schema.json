{
  "$id": "https://github.com/meetnearme/api/architecture/Shared.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "ISOString": {
      "type": "string",
      "format": "date-time"
    },
    "Url": {
      "type": "string",
      "format": "uri",
      "pattern": "^(https?|http?)://",
      "minLength": 1,
      "maxLength": 2000
    },
    "UUID": {
      "type": "string",
      "format": "uuid"
    }
  }
}
