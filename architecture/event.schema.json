{
  "$id": "https://github.com/meetnearme/api/architecture/Event.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "https://github.com/meetnearme/api/architecture/Shared.json#definitions/UUID"
    },
    "ownerId": {
      "type": "string",
      "$ref": "https://github.com/meetnearme/api/architecture/Owner.json"
    },
    "ingestionId": {
      "type": "string",
      "$ref": "https://github.com/meetnearme/api/architecture/Ingestion.json"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "startTime": {
      "$ref": "https://github.com/meetnearme/api/architecture/Shared.json#definitions/ISOString"
    },
    "endTime": {
      "$ref": "https://github.com/meetnearme/api/architecture/Shared.json#definitions/ISOString"
    },
    "recurrenceRule": {
      "type": "string"
    },
    "registrationFields": {
      "items": {
        "type": "string"
      },
      "default": []
    },
    "purchasable": {
      "type": "array",
      "items": {
        "$ref": "https://github.com/meetnearme/api/architecture/Purchasable.json"
      },
      "default": []
    },
    "url": {
      "$ref": "https://github.com/meetnearme/api/architecture/Shared.json#definitions/Url"
    },
    "urlDomain": {
      "type": "string"
    },
    "urlPath": {
      "type": "string"
    },
    "urlQueryParams": {
      "type": "string"
    },
    "urlSlug": {
      "type": "string"
    },
    "imageUrl": {
      "$ref": "https://github.com/meetnearme/api/architecture/Shared.json#definitions/Url"
    },
    "locationLatitude": {
      "type": ["number", "null"]
    },
    "locationLongitude": {
      "type": ["number", "null"]
    },
    "locationAddress": {
      "type": ["string", "null"]
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    },
    "categories": {
      "type": "array",
      "items": {
        "$ref": "https://github.com/meetnearme/api/architecture/Category.json"
      },
      "default": []
    },
    "isExternalEvent": {
      "type": "boolean"
    },
    "zOrderIndex": {
      "type": "string"
    },
    "externalEventUrl": {
      "$ref": "https://github.com/meetnearme/api/architecture/Shared.json#definitions/Url"
    },
    "createdAt": {
      "$ref": "https://github.com/meetnearme/api/architecture/Shared.json#definitions/ISOString"
    },
    "updatedAt": {
      "$ref": "https://github.com/meetnearme/api/architecture/Shared.json#definitions/ISOString"
    },
    "updatedBy": {
      "$ref": "https://github.com/meetnearme/api/architecture/User.json"
    }
  },
  "required": [
    "id",
    "owner",
    "ingestion",
    "name",
    "startTime",
    "url",
    "urlDomain",
    "urlPath",
    "urlSlug",
    "locationLatitude",
    "locationLongitude",
    "locationAddress",
    "locationCountry",
    "locationZipcode",
    "isVerified",
    "isExternalEvent",
    "createdAt",
    "updatedAt",
    "updatedBy"
  ]
}
