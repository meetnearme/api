{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "title": "Event",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "startTime": {
            "type": "string",
            "format": "date-time"
        },
        "longitude": {
            "type": "number"
        },
        "latitude": {
            "type": "number"
        }, 
        "zOrderIndex": {
            "type": "string"
        },
        "title": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "location": {
            "type": "string"
        },
        "url": {
            "type": "string",
            "format": "uri"
        } 
    },
    "required": [
        "id",
        "startTime",
        "latitude",
        "longitude",
        "zOrderIndex",
        "title",
        "location"
    ]
}
