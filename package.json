{
  "name": "meetnearme-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=21.6.0"
  },
  "scripts": {
    "dev": "sst dev",
    "build": "sst build",
    "deploy": "sst deploy",
    "deploy:prod": "sst deploy --stage prod",
    "remove": "sst remove",
    "console": "sst console",
    "serve:static": "npx http-server -o /static -a localhost -p 3001",
    "tailwind:dev": "tailwindcss -i ./static/assets/global.css -o ./static/assets/styles.css --watch",
    "tailwind:prod": "tailwindcss -i ./static/assets/global.css -o ./static/assets/styles.css --minify",
    "templ:watch": "node scripts/watchTempl.js ./functions/lambda/views",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@tsconfig/node18": "^18.2.2",
    "aws-cdk-lib": "2.110.1",
    "constructs": "10.3.0",
    "daisyui": "^4.7.2",
    "node-watch": "^0.7.4",
    "sst": "2.39.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "workspaces": []
}
