{
  "name": "meetnearme-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=21.6.0"
  },
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "build": "sst build",
    "dev:sst": "sst dev",
    "dev:serve-static": "npx http-server -o /static -a localhost -p 3001 --cors",
    "dev:tailwind": "tailwindcss --postcss -i ./static/assets/global.css -o ./static/assets/styles.css --watch",
    "dev:templ": "node scripts/watchTempl.js ./functions/gateway/templates ./functions/lambda/go/seshu/templates",
    "deploy": "sst deploy",
    "deploy:prod": "sst deploy --stage prod",
    "remove": "sst remove",
    "console": "sst console",
    "tailwind:prod": "set NODE_ENV=production && tailwindcss --postcss -i ./static/assets/global.css -o ./static/assets/styles.css --minify",
    "typecheck": "tsc --noEmit",
    "lint:templ:dev": "eslint --ext .templ --fix .",
    "lint:templ:prod": "eslint --ext .templ --max-warnings 0 ."
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.14",
    "@tsconfig/node18": "^18.2.2",
    "aws-cdk-lib": "2.110.1",
    "concurrently": "^8.2.2",
    "constructs": "10.3.0",
    "daisyui": "^4.12.14",
    "eslint": "^8.57.1",
    "eslint-plugin-html": "^7.1.0",
    "node-watch": "^0.7.4",
    "sst": "2.39.5",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.3.3"
  },
  "workspaces": [],
  "dependencies": {
    "@aws-sdk/crc64-nvme-crt": "^3.810.0",
    "@esbuild/win32-x64": "^0.25.4",
    "dotenv": "^16.4.5",
    "kysely": "^0.27.4"
  },
  "eslintConfig": {
    "sourceType": "commonjs"
  }
}
